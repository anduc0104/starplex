<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.cinema.starplex.ui.controllers.admin.MainLayoutController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <BorderPane fx:id="mainBorderPane" layoutX="190.0" layoutY="89.0" prefHeight="600.0" prefWidth="1200.0"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                    AnchorPane.topAnchor="0.0">
            <left>
                <AnchorPane prefHeight="600.0" prefWidth="160.0" style="-fx-background-color: #111827;"
                            BorderPane.alignment="CENTER">
                    <children>
                        <Label layoutX="30.0" layoutY="14.0" text="Welcome" textFill="WHITE">
                            <font>
                                <Font size="24.0"/>
                            </font>
                        </Label>
                        <Label layoutX="45.0" layoutY="49.0" text="Admin" textFill="WHITE">
                            <font>
                                <Font size="24.0"/>
                            </font>
                        </Label>
                        <!--dashboard-->
                        <Button onAction="#dashBoardScene" fx:id="buttonDashBoard" layoutY="140.0"
                                mnemonicParsing="false" prefHeight="45.0" prefWidth="160.0"
                                styleClass="nav-btn" stylesheets="@../../../../css/admin.css" text="Dashboard"
                                textFill="WHITE"/>
                        <Button layoutY="200.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="160.0"
                                styleClass="nav-btn" stylesheets="@../../../../css/admin.css" text="Staff"
                                textFill="WHITE"/>
                        <!--button movie-->
                        <Button onAction="#movieScene" layoutY="320.0" mnemonicParsing="false" prefHeight="45.0"
                                prefWidth="160.0"
                                styleClass="nav-btn" stylesheets="@../../../../css/admin.css" text="Movie"
                                textFill="WHITE"/>
                        <!--button user-->
                        <Button onAction="#managementUser" fx:id="buttonUserManagement" layoutY="260.0"
                                mnemonicParsing="false" prefHeight="45.0" prefWidth="160.0" styleClass="nav-btn"
                                stylesheets="@../../../../css/admin.css" text="User" textFill="WHITE"/>
                        <Button layoutY="380.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="160.0"
                                styleClass="nav-btn" stylesheets="@../../../../css/admin.css" text="Chart"
                                textFill="WHITE"/>
                        <!--button log out-->
                        <Button onAction="#logout" fx:id="buttonLogout" layoutX="49.0" layoutY="491.0"
                                mnemonicParsing="false"
                                style="-fx-background-color: #EF4444; -fx-background-radius: 8px; -fx-cursor: hand;"
                                styleClass="signout-btn" stylesheets="@../../../../css/admin.css"
                                text="Sign out"/>
                    </children>
                </AnchorPane>
            </left>
            <top>

            </top>
            <center>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
