<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.cinema.starplex.ui.controllers.admin.MainLayoutController">
    <children>
        <BorderPane fx:id="mainBorderPane" layoutX="190.0" layoutY="89.0" prefHeight="600.0" prefWidth="1200.0"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                    AnchorPane.topAnchor="0.0">
            <left>
                <AnchorPane prefHeight="600.0" prefWidth="160.0" style="-fx-background-color: #111827;"
                            BorderPane.alignment="CENTER">
                    <children>
                        <Label layoutX="30.0" layoutY="14.0" text="Welcome" textFill="WHITE">
                            <font>
                                <Font size="24.0"/>
                            </font>
                        </Label>
                        <Label layoutX="45.0" layoutY="49.0" text="Admin" textFill="WHITE">
                            <font>
                                <Font size="24.0"/>
                            </font>
                        </Label>
                        <!--button dashboard-->
                        <Button fx:id="buttonDashBoard" layoutY="120.0" mnemonicParsing="false"
                                onAction="#dashBoardScene" prefHeight="45.0" prefWidth="160.0" styleClass="nav-btn"
                                stylesheets="@../../../../css/admin.css" text="Dashboard" textFill="WHITE"/>
                        <!--button staff-->
                        <Button layoutY="170.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="160.0"
                                styleClass="nav-btn" stylesheets="@../../../../css/admin.css" text="Staff"
                                textFill="WHITE"/>
                        <!--button movie-->
                        <Button layoutY="270.0" mnemonicParsing="false" onAction="#movieScene" prefHeight="45.0"
                                prefWidth="160.0" styleClass="nav-btn" stylesheets="@../../../../css/admin.css"
                                text="Movie" textFill="WHITE"/>
                        <!--button user-->
                        <Button fx:id="buttonUserManagement" layoutY="220.0" mnemonicParsing="false"
                                onAction="#managementUser" prefHeight="45.0" prefWidth="160.0" styleClass="nav-btn"
                                stylesheets="@../../../../css/admin.css" text="User" textFill="WHITE"/>
                        <Button layoutY="320.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="160.0"
                                styleClass="nav-btn" stylesheets="@../../../../css/admin.css" text="Chart"
                                textFill="WHITE"/>
                        <!--button log out-->
                        <Button fx:id="buttonLogout" layoutX="50.0" layoutY="449.0" mnemonicParsing="false"
                                onAction="#logout"
                                style="-fx-background-color: #EF4444; -fx-background-radius: 8px; -fx-cursor: hand;"
                                styleClass="signout-btn" stylesheets="@../../../../css/admin.css" text="Sign out"/>
                        <!--button genre-->
                        <Button onAction="#genreScene" layoutY="370.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="160.0"
                                styleClass="nav-btn" stylesheets="@../../../../css/admin.css" text="Genre"
                                textFill="WHITE"/>


                    </children>
                </AnchorPane>
            </left>
            <top>

            </top>
            <center>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
