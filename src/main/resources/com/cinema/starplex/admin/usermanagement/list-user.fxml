<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.text.Text?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.cinema.starplex.ui.controllers.admin.usermanagement.ListUserController"
>
    <Text text="User Management"/>

    <HBox>
        <Button onAction="#addNewScene" layoutX="33.0" layoutY="34.0" mnemonicParsing="false" prefHeight="30.0"
                prefWidth="75.0" styleClass="add-new-btn" stylesheets="@../../../../../css/admin.css"
                text="Add New" textFill="WHITE">
            <font>
                <Font name="System Bold Italic" size="12.0"/>
            </font>
        </Button>
        <TextField layoutX="163.0" layoutY="34.0" prefHeight="30.0"
                   promptText="Search by movie title" styleClass="search"
                   stylesheets="@../../../../../css/admin.css"/>
    </HBox>
    <TableView fx:id="tableView" styleClass="table" stylesheets="@../../../../../css/admin.css">
        <columns>
            <TableColumn fx:id="idColumn" text="ID"/>
            <TableColumn fx:id="fullNameColumn" text="Full Name"/>
            <TableColumn fx:id="usernameColumn" text="Username"/>
            <TableColumn fx:id="mailColumn" text="Mail"/>
            <TableColumn fx:id="phoneColumn" text="Phone"/>
            <TableColumn fx:id="roleColumn" text="Role"/>
            <TableColumn fx:id="actionColumn" text="Action"/>
        </columns>
    </TableView>
</VBox>
